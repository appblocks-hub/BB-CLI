    "cli-progress": "3.10.0",
    "cli-table3": "^0.6.3",
    "commander": "8.2.0",
    "conf": "10.1.1",
    "decompress": "^4.2.1",
    "dotenv": "10.0.0",
    "emoji-regex": "^10.1.0",
    "figlet": "1.5.2",
    "figures": "3.2.0",
